{UTF-8}
alpha=
	1
	~	1 / (People/Day)
	~		|

BaseIFR=
	0.005
	~	dmnl
	~		|

beta=
	0.15
	~	1/Day
	~		|

Deaths=
	Infectious / DiseaseDuration * BaseIFR
	~	People/Days
	~		|

DiseaseDuration=
	10
	~	Days
	~		|

dn=
	SMOOTH N(Deaths, PMean, 0, PMeanOrder)
	~	People/Days
	~		|

g death=
	EXP(-alpha * dn)
	~	dmnl
	~		|

inf exp=
	beta * Infectious * g death * SFrac
	~	People/Days
	~		|

Infections=
	inf exp
	~	People/Days
	~		|

Infectious= INTEG (
	Infections-Deaths-Recoveries,
		1)
	~	People
	~		|

Initial susceptible=
	1e+06
	~	People
	~		|

PMean=
	10
	~	Days
	~		|

PMeanOrder=
	2
	~	dmnl
	~		|

Re=
	beta * DiseaseDuration * g death * SFrac
	~	dmnl
	~		|

Recovered= INTEG (
	Recoveries,
		0)
	~	People
	~		|

Recoveries=
	Infectious / DiseaseDuration * (1-BaseIFR)
	~	People/Days
	~		|

SFrac=
	Susceptible / Total population
	~	dmnl
	~		|

Susceptible= INTEG (
	-Infections,
		Initial susceptible)
	~	People
	~		|

Total population=
	Susceptible + Infectious + Recovered
	~	People
	~		|

